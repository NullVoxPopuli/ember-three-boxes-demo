<main>
  <Demo::Ember::Scene @onInit={{this.animate}} as |s|>
    <s.DirectionalLight @x={{-5}} @y={{0}} @z={{-10}} />
    <s.PerspectiveCamera @z={{3.2}} @aspectRatio={{this.aspectRatio}} />

    {{#each this.appState.rotations as |rotation|}}
      <s.Box @rotation={{rotation.r}} />
    {{/each}}

  </Demo::Ember::Scene>
</main>